<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form">
	<Dialog id="dialog" confirm="onConfirm" title="{i18n>Interpret_CreateRun}">
		<content>
			<f:SimpleForm>
				<f:content>
					<mvc:XMLView viewName="sap.secmon.ui.loglearning.dialog.CreateRunDialog"
						id="view" />
				</f:content>
			</f:SimpleForm>
		</content>
		<buttons>
			<Button text="{i18n>Interpret_Create}" press="onCloseDialog"
				id="ok" />
			<Button text="{i18n>Interpret_Close}" press="onCloseDialog"
				id="cancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>