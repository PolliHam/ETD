<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Dialog confirm="onConfirm" title="{i18n>Interpret_TimeRange}">
		<content>
			<mvc:XMLView
				viewName="sap.secmon.ui.loglearning.log.TimeRangeFilterDialog"
				id="view" />
		</content>
		<buttons>
			<Button text="{i18n>BU_BUT_OK}" press="onCloseTimeRangeFilterDialog"
				id="ok" />
			<Button text="{i18n>BU_BUT_Cancel}" press="onCloseTimeRangeFilterDialog"
				id="cancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>