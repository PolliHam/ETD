<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<ViewSettingsDialog confirm="onConfirm">
		<sortItems>
			<ViewSettingsItem key="ObjectType" text="{i18n>CD_ObjType}" />
			<ViewSettingsItem key="ObjectName" text="{i18n>CD_ObjName}"
				selected="true" />
			<ViewSettingsItem key="ObjectNamespace" text="{i18n>CD_ObjNamespace}" />
			<ViewSettingsItem key="Description" text="{i18n>CD_Description}" />
			<ViewSettingsItem key="Version" text="{i18n>CD_Version}" />
		</sortItems>
		<groupItems>
			<ViewSettingsItem key="ObjectType" text="{i18n>CD_ObjType}"
				selected="true" />
			<ViewSettingsItem key="ObjectNamespace" text="{i18n>CD_ObjNamespace}" />
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>